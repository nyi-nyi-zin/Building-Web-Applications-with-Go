{{template "base" . }}

{{define "title"}}
    Payment Succeeded!
{{end}}

{{define "content"}}
    <h2 class="mt-5">Payment Succeeded</h2>
    <hr>
    <p>Payment Intent: {{.Data.pi}}</p>
    <p>Cardholder: {{.Data.cardholder}}</p>
    <p>Email: {{.Data.email}}</p>
    <p>Payment Method: {{.Data.pm}}</p>
    <p>Payment Amount: {{.Data.pa}}</p>
    <p>Currency: {{.Data.pc}}</p>
{{end}}